let athleteFinalPosition = 'first place';
switch(athleteFinalPosition){
  case 'first place':
    console.log('You won the gold medal!');
    break;
  case 'second place':
    console.log('You won the silver medal!');
    break;
  case 'third place':
    console.log('You won the bronze medal!');
    break;
  default:
    console.log('No medal awarded.');
    break;
}
